a {
    color: #000000;
    font-size: 20px;
}

.form-login {
    margin: auto;
    max-width: 420px;
    border: 1px solid gray;
    border-radius: 4px;
}

/*Header CSS*/
.navbar {
    background: #FF6633;
    color: white;
    margin-bottom: 5px;
}

.navbar .navbar-brand {
    padding: 7px;
}

.navbar .navbar-header {
    min-width: 160px;
}

.navbar .navbar-nav .item {
    border-left: 1px solid rgba(245, 245, 245, 0.75);
    margin-top: 8px;
    padding: 8px 15px 0px 15px;
    height: 35px;
}

.navbar .product-logo .main-text {
    font-size: 24px;
    text-align: center;
    padding-top: 10px;
    margin-right: 5px;
}

.navbar .product-logo .main-text .tm-text {
    font-size: 12px;
    float: right;
}

.navbar-default .navbar-nav > li > a {
    color: white;
}

@media (min-width: 768px) {
    .navbar-nav > li {
        float: left
    }

    .navbar-nav > li > a {
        padding-top: 15px;
        padding-bottom: 15px
    }
}

